<Window x:Class="Orden.Views.Order"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Converts="clr-namespace:Orden.Converts"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"   
        mc:Ignorable="d"
        Icon="/Img/favicon.ico"
        FontFamily="Arial" 
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="11"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        StateChanged="Window_StateChanged"
        TextElement.FontFamily="Arial Rounded MT Bold" 
        Width="2200" 
        Height="5000"
        DataContext="ModelOrder"
        Closing="Window_Closing">
    <Window.Resources>
        <Converts:StringToNumber x:Key="ToNumber"/>
        <Style x:Key="StyleParticipants" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="Width" Value="310"></Setter>
        </Style>
        <Style x:Key="StyleInsurance" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="Width" Value="200"></Setter>
        </Style>
        <Style x:Key="StyleContingency" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="Width" Value="400"></Setter>
        </Style>
        <Style x:Key="StyleStranded" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="Width" Value="310"></Setter>
        </Style>
        <Style x:Key="StyleDebit" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="Width" Value="400"></Setter>
        </Style>
        <Style x:Key="txtStyle" TargetType="TextBox" BasedOn="{StaticResource MaterialDesignOutlinedTextFieldTextBox}">
            <Setter Property="Height" Value="Auto" />
        </Style>
        <Style x:Key="cbStyle" TargetType="ComboBox" BasedOn="{StaticResource MaterialDesignFloatingHintComboBox}">
            <Setter Property="Height" Value="45" />
        </Style>
        <Style x:Key="dpStyle" TargetType="DatePicker" BasedOn="{StaticResource MaterialDesignFloatingHintDatePicker}">
            <Setter Property="Height" Value="Auto" />
        </Style>
    </Window.Resources>
    <Grid Background="White" Margin="20" Name="Main" HorizontalAlignment="Center" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ScrollViewer x:Name="scrollHome"  VerticalScrollBarVisibility="Visible"  HorizontalScrollBarVisibility="Visible" TextElement.Foreground="Black" Background="white" Width="auto" Grid.Row="1" Grid.Column="1">
            <StackPanel>
                <Grid Background="White" Margin="30 10 20 -10" Name="gridHome">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <TextBox Width="250" Style="{StaticResource MaterialDesignFloatingHintTextBox}" CharacterCasing="Upper" Margin="20 20 0 15" Name="txtCase" Grid.Column="0" Grid.Row="1"  materialDesign:HintAssist.Hint="CASO BIZAGI"/>
                    <Button Content="{materialDesign:PackIcon Search}" ToolTip="Buscar Caso" Grid.Row="1" Grid.Column="1"  Margin="35 30 0 15" Name="FindCase" Click="FindCase_Click"/>
                    <ComboBox x:Name="cbStateOrder" Margin="20 0 0 0" Style="{StaticResource cbStyle}" Grid.Row="1" Width="170" Grid.Column="2" materialDesign:HintAssist.Hint="ESTADOS DISPONIBLES" ItemsSource="{Binding NameState}" DisplayMemberPath="NameState"  SelectedValuePath="IdState" IsEditable="False" IsReadOnly="True"/>
                    <ComboBox x:Name="cbCaseReversed" Margin="35 0 0 0" Style="{StaticResource cbStyle}" Grid.Row="1"  Grid.Column="3" materialDesign:HintAssist.Hint="CASO REVERSADO" Width="130" IsEditable="False" IsReadOnly="True"/>
                    <Button Style="{StaticResource MaterialDesignFlatDarkBgButton}" Grid.Row="1" Grid.Column="4" HorizontalAlignment="Left"  Margin="15 30 0 15" Name="CleanSheet" Click="CleanSheet_Click">Limpiar Planilla</Button>
                    <Button Style="{StaticResource MaterialDesignFlatDarkBgButton}" Visibility="Collapsed" Grid.Row="1" Grid.Column="5" HorizontalAlignment="Left"  Margin="20 30 0 15" Name="Calculate">Calcular TRC</Button>
                    <Button Style="{StaticResource MaterialDesignFlatDarkBgButton}" Grid.Column="6" HorizontalAlignment="Left"  Margin="30,30,0,15" Name="Save" Click="Save_Click" Grid.ColumnSpan="2">Guardar Cambios</Button>
                    <Button Content="{materialDesign:PackIcon Camera}" ToolTip="Imprimir caso" Grid.Row="1" Grid.Column="7" HorizontalAlignment="Left"  Margin="15 30 0 15" Name="Print" Click="Print_Click" Visibility="Hidden"></Button>
                </Grid>
                <Grid Background="White" Margin="40 20 20 20" Name="gridParticipants" HorizontalAlignment="Center" Width="1265">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Margin="50 0 0 0" Grid.ColumnSpan="7" HorizontalAlignment="Center">SOLICITANTES Y RELACIONADOS DEL CRÉDITO</Label>
                    <ComboBox x:Name="cbType"  Text="{Binding IdentificationType, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="220" Margin="10 20 0 20" Grid.Row="1" Grid.Column="0" materialDesign:HintAssist.Hint="TIPO DOCUMENTO"  IsEditable="False" IsReadOnly="False"/>
                    <TextBox Name="txtIdParticipant"  Text="{Binding IdentificationNumber, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource ToNumber}}" TextChanged="TxtIdParticipant_TextChanged"  Margin="30 20 0 20" Grid.Row="1" Grid.Column="1" materialDesign:HintAssist.Hint="NUMERO DOCUMENTO"></TextBox>
                    <TextBox Name="txtNameParticipant" CharacterCasing="Upper" Text="{Binding NameParticipant, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Margin="20 20 20 20" Width="400" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" materialDesign:HintAssist.Hint="NOMBRE PARTICIPANTE"></TextBox>
                    <ComboBox Name="cbTypeParticipant" Text="{Binding TypeParticipant, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Margin="0 20 0 20" Grid.Row="1" Grid.Column="4" materialDesign:HintAssist.Hint="TIPO PARTICIPANTE" IsEditable="False" IsReadOnly="False"/>
                    <Button Content="{materialDesign:PackIcon Kind=ContentSaveAll}" Style="{StaticResource MaterialDesignFlatAccentButton}" ToolTip="Guardar" Grid.Row="1" Grid.Column="5" Margin="30 -2 0 0" Name="saveParticipant" Click="SaveParticipant_Click"></Button>
                    <Button Content="{materialDesign:PackIcon Kind=AddCircle}" Style="{StaticResource MaterialDesignFlatAccentButton}" ToolTip="Limpiar" Grid.Row="1" Grid.Column="6" Margin="30 -2 0 0" Name="NewParticipant" Click="NewParticipant_Click"></Button>
                    <DataGrid x:Name="datagridParticipant" 
                              Margin="10 0 0 0"
                              FontSize="11"
                              CanUserResizeColumns="False"  
                              ItemsSource="{Binding Participants}"
                              ColumnHeaderStyle="{StaticResource StyleParticipants}" 
                              CommandManager.PreviewCanExecute="DatagridParticipant_PreviewCanExecute" 
                              GotFocus="DatagridParticipant_GotFocus"
                              HorizontalAlignment="Center" Grid.Column="0" Grid.Row="2" Height="auto" 
                              Grid.ColumnSpan="7" CanUserAddRows="False" IsReadOnly="False" AutoGenerateColumns="False"
                              SelectionChanged="DatagridParticipant_SelectionChanged">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="TIPO DOCUMENTO" Binding="{Binding IdParticipant}" Visibility="Collapsed"></DataGridTextColumn>
                            <DataGridTextColumn Header="TIPO DOCUMENTO" Binding="{Binding IdentificationType}"></DataGridTextColumn>
                            <DataGridTextColumn Header="NUMERO DE DOCUMENTO" Binding="{Binding IdentificationNumber}"></DataGridTextColumn>
                            <DataGridTextColumn Header="NOMBRE COMPLETO" Binding="{Binding NameParticipant}"></DataGridTextColumn>
                            <DataGridTextColumn Header="TIPO PARTICIPANTE" Binding="{Binding TypeParticipant}"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
                <Grid>
                    <Grid x:Name="gridGeneral" HorizontalAlignment="Center" Margin="20 -25 20 0" VerticalAlignment="Center" Width="1265">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="130"/>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition Width="40"/>
                            <ColumnDefinition Width="35"/>
                            <ColumnDefinition Width="185"/>
                            <ColumnDefinition Width="220"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" Grid.Column="0" Margin="121,0,21,0" Grid.ColumnSpan="2" HorizontalAlignment="Center">INFORMACIÓN GENERAL</Label>
                        <!--INFORMACIÓN GENERAL-->
                        <TextBox Name="txtIdGeneral"  Text="{Binding IdTicket, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="1" Visibility="Collapsed"/>
                        <TextBox Name="txtFinancing" Text="{Binding Financing, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsEnabled="True" IsReadOnly="True" Background="LightGreen" Width="350" ToolTip="Porcentaje Financiación"  Style="{StaticResource txtStyle}" Height="37" Margin="10,10,0,0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="1" materialDesign:HintAssist.Hint="% FINANCIACIÓN" VerticalAlignment="Top"/>
                        <ComboBox Name="cbClassification" DropDownClosed="CbClassification_DropDownClosed" Focusable="False" Text="{Binding Rating, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource cbStyle}" Width="350" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="2" materialDesign:HintAssist.Hint="CLASIFICACIÓN CARTERA"/>
                        <ComboBox Name="cbBussines" DropDownClosed="CbBussines_DropDownClosed" Focusable="False"  Text="{Binding Cession,  Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource cbStyle}" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="3" materialDesign:HintAssist.Hint="NEGOCIO COMPRA (CESIÓN)"/>
                        <ComboBox Name="cbScheme" DropDownClosed="CbScheme_DropDownClosed" Focusable="False" Text="{Binding Scheme, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource cbStyle}" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="4" materialDesign:HintAssist.Hint="SCHEME CODE (Código)"/>
                        <ComboBox Name="cbVda" DropDownClosed="CbVda_DropDownClosed" Focusable="False"  Text="{Binding Vda, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource cbStyle}" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="5" materialDesign:HintAssist.Hint="VDA NUEVA O USADA"/>
                        <TextBox Name="txtSolId" TextChanged="TxtSolId_TextChanged" KeyDown="TxtSolId_KeyDown" LostFocus="TxtSolId_LostFocus" Text="{Binding Office,Mode=TwoWay}" Width="350" Style="{StaticResource txtStyle}" Margin="-40,10,170,0" Grid.ColumnSpan="6" Grid.Column="0" Grid.Row="6" materialDesign:HintAssist.Hint="SOL ID (Oficina)"/>
                        <TextBox Name="txtModality" TextChanged="TxtModality_TextChanged" IsReadOnly="True" IsEnabled="True" Text="{Binding Modality, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Background="LightGreen" Width="350" Style="{StaticResource txtStyle}" Margin="-40,10,170,0" Grid.ColumnSpan="6" Grid.Column="0" Grid.Row="7" materialDesign:HintAssist.Hint="MODALIDAD "/>
                        <TextBox Name="txtValueApprove" KeyDown="TxtValueApprove_KeyDown" LostFocus="TxtValueApprove_LostFocus" Text="{Binding ApprovedValue, Mode=TwoWay, Converter={StaticResource ToNumber}, StringFormat=C}" Width="350"  Style="{StaticResource txtStyle}" Margin="-40,10,170,0" Grid.ColumnSpan="6" Grid.Column="0" Grid.Row="8" materialDesign:HintAssist.Hint="VALOR APROBADO"/>
                        <TextBox Name="txtTerm" LostFocus="TxtTerm_LostFocus" GotFocus="TxtTerm_GotFocus" KeyDown="TxtTerm_KeyDown" Text="{Binding Term, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource txtStyle}" Margin="-40,10,170,0" Grid.ColumnSpan="6" Grid.Column="0" Grid.Row="9" materialDesign:HintAssist.Hint="PLAZO"/>
                        <ComboBox Name="cbCoin" DropDownClosed="CbCoin_DropDownClosed" Focusable="False" Text="{Binding Currency, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource cbStyle}" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="10" materialDesign:HintAssist.Hint="MONEDA"/>
                        <ComboBox Name="cbPlan" DropDownClosed="CbPlan_DropDownClosed" Focusable="False" Text="{Binding Plan, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350"  Style="{StaticResource cbStyle}" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="11" materialDesign:HintAssist.Hint="PLAN"/>
                        <DatePicker Name="dpDateApprove" SelectedDate="{Binding ApprovedDate, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" IsEnabled="True" Style="{StaticResource dpStyle}" Margin="10 10 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="12" materialDesign:HintAssist.Hint="FECHA DE APROBACIÓN "/>
                        <TextBox Name="txtVtoApprove" IsEnabled="True" IsReadOnly="True" Text="{Binding ApprovedVto, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350"  Style="{StaticResource txtStyle}" Margin="-40 10 170 0" Grid.ColumnSpan="6" Grid.Column="0" Grid.Row="13" materialDesign:HintAssist.Hint="VTO. APROBACIÓN"/>
                        <DatePicker Name="dpDateRevived" LostFocus="DpDateRevived_LostFocus" SelectedDate="{Binding RevivedDate, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350"  Style="{StaticResource dpStyle}" Margin="10 10 0 10" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="14" materialDesign:HintAssist.Hint="FECHA DE REVIVIDO"/>
                        <ComboBox Name="cbSegment" Focusable="False" Text="{Binding Segment, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350"  Style="{StaticResource cbStyle}" Margin="10 -5 0 0" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="15" materialDesign:HintAssist.Hint="SEGMENTO CLIENTE"/>
                        <TextBox Name="txtRate" KeyDown="TxtRate_KeyDown" LostFocus="TxtRate_LostFocus" IsEnabled="True" IsReadOnly="False" Text="{Binding Rate, Mode=TwoWay, Converter={StaticResource ToNumber}}" Width="350"  Style="{StaticResource txtStyle}" Margin="10,12,0,220" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="16" materialDesign:HintAssist.Hint="TASA PxR" Height="37" VerticalAlignment="Bottom"/>
                    </Grid>
                    <Grid x:Name="gridFrech" HorizontalAlignment="Center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="220"/>
                            <ColumnDefinition Width="220"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" Grid.Column="2" Margin="20 0 0 0" Grid.ColumnSpan="2" HorizontalAlignment="Center">VALIDACIÓN FRECH</Label>
                        <!--VALIDACIÓN FRECH-->
                        <TextBox Name="txtIdFrech"  Text="{Binding IdTicket, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="1" Visibility="Collapsed"/>
                        <TextBox Name="txtIncome" LostFocus="TxtIncome_LostFocus"  KeyDown="TxtIncome_KeyDown" Text="{Binding Income, Mode=TwoWay, Converter={StaticResource ToNumber}, StringFormat=C}"  Width="350" Style="{StaticResource txtStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="1" materialDesign:HintAssist.Hint="INGRESOS"/>
                        <ComboBox Name="cbFrech" DropDownClosed="CbFrech_DropDownClosed" Focusable="False"  Text="{Binding HasFrech, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource cbStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="2" materialDesign:HintAssist.Hint="POSEE FRECH "/>
                        <ComboBox Name="cbFonvivienda" DropDownClosed="CbFonvivienda_DropDownClosed" Focusable="False" Text="{Binding HasFonvivienda, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource cbStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="3" materialDesign:HintAssist.Hint="FONVIVIENDA"/>
                        <ComboBox Name="cbFormat" DropDownClosed="CbFormat_DropDownClosed" Focusable="False" Text="{Binding Format, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  Width="350" Style="{StaticResource cbStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="4" materialDesign:HintAssist.Hint="FORMATO COBERTURA"/>
                        <TextBox Name="txtSubsidy" IsReadOnly="True" IsEnabled="True" Text="NO" Width="350" Style="{StaticResource txtStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="5" materialDesign:HintAssist.Hint="SUBSIDIO EXTENDIDO"/>
                        <TextBox Name="txtFrechResult" GotFocus="TxtFrechResult_GotFocus" IsReadOnly="True" Text="{Binding ResultFrech, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="350" Style="{StaticResource txtStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="6" materialDesign:HintAssist.Hint="RESULTADO FRECH (APLICA)"/>
                        <ComboBox Name="cbPlace" DropDownOpened="CbPlace_DropDownOpened" Focusable="False" DropDownClosed="CbPlace_DropDownClosed" Width="350" Style="{StaticResource cbStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="7" materialDesign:HintAssist.Hint="VIVIENDA SOSTENIBLE" Visibility="Hidden"/>
                        <ComboBox Name="cbFrechType" DropDownClosed="CbFrechType_DropDownClosed" Focusable="False" Width="350" Style="{StaticResource cbStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="7" materialDesign:HintAssist.Hint="TIPO DE FRECH"/>
                        <TextBox Name="txtPoints" IsReadOnly="True" Width="350" Style="{StaticResource txtStyle}" Text="{Binding points, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource ToNumber}}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="8" materialDesign:HintAssist.Hint="PUNTOS COBERTURA"/>
                        <TextBox Name="txtMCY" IsReadOnly="True" KeyDown="TxtMCY_KeyDown" Text="{Binding YearSubsidy, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource ToNumber}}" Width="350" Style="{StaticResource txtStyle}" Margin="0,10,-30,0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="9" materialDesign:HintAssist.Hint="Año Asignación MCY"/>
                        <TextBox Name="txtDelivery" Width="350" Style="{StaticResource txtStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="10" materialDesign:HintAssist.Hint="ENTREGA No."/>
                        <TextBox Name="txtValueDelivery" KeyDown="TxtValueDelivery_KeyDown" LostFocus="TxtValueDelivery_LostFocus" Width="350" Style="{StaticResource txtStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="11" materialDesign:HintAssist.Hint="VALOR ENTREGA"/>
                        <ComboBox Name="cbBRP" Focusable="False" Width="350" Style="{StaticResource cbStyle}" Margin="30 10 0 0" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="12" materialDesign:HintAssist.Hint="BRP" Height="46"/>
                    </Grid>
                    <Grid x:Name="gridProperty" HorizontalAlignment="Center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="220"/>
                            <ColumnDefinition Width="220"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" Grid.Column="4" Margin="60 0 0 0" Grid.ColumnSpan="2" HorizontalAlignment="Center">INFORMACIÓN INMUEBLE (AVALUO)</Label>
                        <!--INFORMACIÓN INMUEBLE (AVALUO)	-->
                        <TextBox Name="txtIdProperty"  Text="{Binding IdTicket, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="1" Visibility="Collapsed"/>
                        <DatePicker Name="dpDateAppraisal" LostFocus="DpDateAppraisal_LostFocus" SelectedDate="{Binding AppraisalDate, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="340" Style="{StaticResource dpStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="1" materialDesign:HintAssist.Hint="FECHA AVALUO "/>
                        <ComboBox Name="cbPropertyType" DropDownClosed="CbPropertyType_DropDownClosed" Focusable="False" Text="{Binding TypeProperty, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="340" Style="{StaticResource cbStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="2" materialDesign:HintAssist.Hint="TIPO INMUEBLE"/>
                        <ComboBox Name="cbDeparment" DropDownClosed="CbDeparment_DropDownClosed" Focusable="False" Text="{Binding Deparment, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="340" Style="{StaticResource cbStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="3" materialDesign:HintAssist.Hint="DEPARTAMENTO"/>
                        <ComboBox Name="cbCity" DropDownClosed="CbCity_DropDownClosed" Focusable="False" Text="{Binding City, Mode=OneWayToSource,UpdateSourceTrigger=PropertyChanged}" Width="340" Style="{StaticResource cbStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="4" materialDesign:HintAssist.Hint="CIUDAD"/>
                        <TextBox Name="txtDirection" Text="{Binding PropertyAddress, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="340" Style="{StaticResource txtStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="5" TextWrapping="Wrap"  MaxLines="4" ScrollViewer.VerticalScrollBarVisibility="Auto" materialDesign:HintAssist.Hint="DIRECCION INMUEBLE"/>
                        <ComboBox Name="cbClass" DropDownClosed="CbClass_DropDownClosed" Text="{Binding WarrantyClass, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="340" Style="{StaticResource cbStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="6" materialDesign:HintAssist.Hint="CLASE GARANTÍA "/>
                        <TextBox Name="txtAppraisalValue" KeyDown="TxtAppraisalValue_KeyDown" LostFocus="TxtAppraisalValue_LostFocus" Text="{Binding AppraisalValue, Mode=TwoWay, Converter={StaticResource ToNumber}, StringFormat=C}" Width="340" Style="{StaticResource txtStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="7" materialDesign:HintAssist.Hint="VALOR AVALUO "/>
                        <TextBox Name="txtCosts" KeyDown="TxtCosts_KeyDown" LostFocus="TxtCosts_LostFocus" Text="{Binding Costs, Mode=TwoWay, Converter={StaticResource ToNumber},  StringFormat=C}" Width="340" Style="{StaticResource txtStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="8" materialDesign:HintAssist.Hint="COSTOS DIRECTOS"/>
                        <TextBox Name="txtValueTRC" KeyDown="TxtValueTRC_KeyDown" LostFocus="TxtValueTRC_LostFocus" Text="{Binding ValueTrc, Mode=TwoWay, Converter={StaticResource ToNumber},  StringFormat=C}" Width="340" Style="{StaticResource txtStyle}" Margin="60 10 0 0" Grid.ColumnSpan="2" Grid.Column="4" Grid.Row="9" materialDesign:HintAssist.Hint="VALOR PRIMA TRC"/>
                    </Grid>
                    <Grid x:Name="gridFinacle" HorizontalAlignment="Center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
